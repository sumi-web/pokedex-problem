datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

model Pokemon {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  type      String[]
  sprite    String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
