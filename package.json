{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prisma:generate && cross-env NODE_ENV=development && next dev",
    "build": "npm run prisma:migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.14",
    "@prisma/client": "^5.12.1",
    "@tanstack/react-query": "^5.28.14",
    "@trpc/client": "^11.0.0-rc.330",
    "@trpc/next": "^11.0.0-rc.330",
    "@trpc/react-query": "^11.0.0-rc.330",
    "@trpc/server": "^11.0.0-rc.330",
    "@vercel/postgres": "^0.8.0",
    "cross-env": "^7.0.3",
    "next": "14.1.4",
    "prisma": "^5.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^2.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "typescript": "^5"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  }
}
